{
  "_type": "Capability",
  "name": "user",
  "partition": "com.nokia.qtjsondb.User",
  "accessRules": {
    "rw": {
      "read": [".*"],
      "write": ["[?_type startsWith \"public.\"]", "[?_type startsWith \"%domain.\"]",
                "[?_owner=\"%owner\"]"]
    },
    "setOwner": {
      "setOwner": ["[?_type startsWith \"public.\"]", "[?_type startsWith \"%domain.\"]",
                   "[?_owner=\"%owner\"]"]
    }
  }
}
