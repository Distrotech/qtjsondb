{
  "_type": "Capability",
  "name": "system",
  "partition": "all",
  "accessRules": {
    "setOwner": {
      "read": [".*"],
      "write": [".*"],
      "setOwner": [".*"]
    }
  }
}
{
  "_type": "Capability",
  "name": "system",
  "partition": "com.nokia.qtjsondb.System",
  "accessRules": {
    "rw": {
      "read": [".*"],
      "write": ["[?_type startsWith \"public.\"]", "[?_type startsWith \"%domain.\"]",
                "[?_owner=\"%owner\"]"]
    }
  }
}
